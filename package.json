{
  "name": "juice-toast",
  "version": "1.3.3",
  "description": "Lightweight, dependency-free toast notification library",
  "keywords": [
    "toast",
    "notification",
    "ui",
    "javascript",
    "frontend",
    "browser",
    "vanilla-js"
  ],
  "license": "MIT",
  "author": "KhairyK",
  "homepage": "https://github.com/KhairyK/juiceToast",
  "bugs": {
    "url": "https://github.com/KhairyK/juiceToast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhairyK/juiceToast.git"
  },
  "funding": "https://patreon.com/Khairy47",
  "engines": {
    "node": ">=19"
  },
  "scripts": {
    "format": "npx prettier --write src/"
  },
  "module": "dist/juice-toast.esm.js",
  "types": "dist/juice-toast.d.ts",
  "exports": {
    ".": {
      "import": "./dist/juice-toast.esm.js",
      "types": "./dist/juice-toast.d.ts"
    },
    "./ios": {
      "import": "./dist/juice-toast.ios.js",
      "types": "./dist/juice-toast-ios.d.ts"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "WARNER_NEXT_VERSION.md",
    "EoL.md",
    "NEW_FUNCTION.md",
    "API.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "unpkg": "dist/juice-toast.esm.js",
  "jsdelivr": "dist/juice-toast.esm.js",
  "devDependencies": {
    "prettier": "^3.8.1"
  }
}
